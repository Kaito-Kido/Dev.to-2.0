if (`<%= raw @react.reactable_type%>` == "Post") {
  $("a[data-post-id=<%=@reactable.id%>]").replaceWith(
    `<%= j render "reacts/reacted", reactable: @reactable %>`
  ) 
}
else {
  $("a[data-comment-id=<%=@reactable.id%>]").replaceWith(
    `<%= j render "reacts/reacted", reactable: @reactable %>`
  ) 
}

